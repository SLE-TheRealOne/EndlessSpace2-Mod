    <!--
    Each boos should have 3 GUI elements
      * An ExtendedGuiElement for the construction item
      * A GuiElement for the effect itself where it applies
      * A GuiElement to indicate that boost is active on StarSystem screen
    -->

    <!-- STRAT 1 -->
    <ExtendedGuiElement Name="StrategicResourcesUsage1">
        <Title>%StrategicResourcesUsage1_title</Title>
        <Description>%StrategicResourcesUsage1_description</Description>
        <Icons>
            <Icon Size="Small" Path="Gui/Strategic1" />
            <Icon Size="Large" Path="Bitmaps/Dynamic/Strategics/Strategic01Large" />
        </Icons>
        <TemporaryEffectTooltipElement TemporaryEffectName="StrategicResourcesUsage1" >
            <EffectOverride>%StrategicResourcesUsage1_tooltip</EffectOverride>
        </TemporaryEffectTooltipElement>
    </ExtendedGuiElement>

    <GuiElement Name="StrategicResourcesUsage1Effect" >
        <Title>%StrategicResourcesUsage1_effect</Title>
    </GuiElement>

    <GuiElement Name="TemporaryEffectStrategicResourcesUsage1">
